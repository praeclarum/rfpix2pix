{
    "type": "RFPix2pixModel",
    "max_size": 128,
    "sample_batch_size": 8,
    "train_batch_size": 48,
    "train_minibatch_size": 48,
    "train_images": 1000000,
    "learning_rate": 0.0002,
    "num_inference_steps": 12,
    "timestep_sampling": "logit-normal",
    "saliency_accuracy_threshold": 0.9998,
    "saliency_warmup_threshold": 0.90,
    "velocity_net": {
        "type": "UNet",
        "model_channels": 64,
        "ch_mult": [1, 2, 4, 4],
        "normalization": "GroupNorm32",
        "activation": "SiLU",
        "num_res_blocks": 2,
        "zero_res_blocks": false
    },
    "saliency_net": {
        "type": "ResNetSaliencyNet",
        "backbone": "resnet18",
        "num_classes": 2,
        "pretrained": true,
        "latent_channels": 512
    }
}
