{
    "type": "RFPix2pixModel",
    "max_size": 128,
    "domain0": [],
    "domain1": [],
    "codec": {
        "type": "Codec",
        "net": {
            "type": "VAE",
            "latent_channels": 8,
            "model_channels": 64,
            "ch_mult": [
                1,
                2,
                4,
                4
            ],
            "normalization": "GroupNorm32",
            "activation": "SiLU",
            "num_res_blocks": 2,
            "decoder_num_res_blocks": 3,
            "mid_attention": true,
            "num_attention_heads": 1,
            "downsample": "StridedConv",
            "upsample": "NearestConv",
            "output_activation": "Tanh"
        },
        "losses": [
            {
                "type": "L1Loss",
                "id": "recon_loss",
                "weight": 1.0
            },
            {
                "type": "LPIPSLoss",
                "id": "lpips_loss",
                "weight": 1.0
            },
            {
                "type": "KLLoss",
                "id": "kl_loss",
                "weight": 1e-03
            }
        ],
        "gradient_clip": 1.0,
        "learning_rate": 0.0001,
        "train_batch_size": 64,
        "train_minibatch_size": 32,
        "train_images": 1000000,
        "sample_batch_size": 8,
        "augmentations": [
            "hflip",
            "color_jitter"
        ]
    },
    "velocity": {
        "type": "Velocity",
        "net": {
            "type": "DiT",
            "patch_size": 2,
            "hidden_size": 768,
            "depth": 12,
            "num_heads": 12,
            "mlp_ratio": 4.0
        },
        "sample_batch_size": 8,
        "train_batch_size": 64,
        "train_minibatch_size": 64,
        "train_images": 30000000,
        "learning_rate": 0.0002,
        "lr_schedule": "cosine",
        "warmup_images": 500000,
        "gradient_clip": 1.0,
        "ema": 0.9999,
        "num_inference_steps": 12,
        "timestep_sampling": "logit-normal",
        "structure_pairing": false,
        "structure_candidates": 8
    },
    "saliency": {
        "type": "Saliency",
        "net": {
            "type": "ResNetSaliencyNet",
            "backbone": "resnet34",
            "num_classes": 2,
            "pretrained": true,
            "latent_channels": 512
        },
        "learning_rate": 5e-05,
        "accuracy_threshold": 0.995,
        "warmup_threshold": 0.9,
        "blend_fraction": 0.0,
        "label_smoothing": 0.1,
        "augmentations": [
            "color_jitter",
            "grayscale",
            "hflip",
            "random_erasing"
        ]
    }
}
