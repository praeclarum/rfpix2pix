{
    "type": "RFPix2pixModel",
    "max_size": 128,
    "domain0": [],
    "domain1": [],
    "codec": {
        "type": "Codec",
        "net": {
            "type": "ConvEncoderDecoder",
            "latent_channels": 16,
            "model_channels": 64,
            "ch_mult": [
                1,
                2,
                4,
                8
            ],
            "normalization": "GroupNorm32",
            "activation": "SiLU",
            "num_res_blocks": 2
        },
        "losses": [
            {
                "type": "L1Loss",
                "id": "recon_loss",
                "weight": 1.0
            },
            {
                "type": "LPIPSLoss",
                "id": "lpips_loss",
                "weight": 1.0
            },
            {
                "type": "KLLoss",
                "id": "kl_loss",
                "weight": 1e-06
            }
        ],
        "sample_batch_size": 8,
        "train_batch_size": 48,
        "train_minibatch_size": 24,
        "train_images": 2000000,
        "learning_rate": 0.0001,
        "augmentations": [
            "hflip"
        ]
    },
    "velocity": {
        "type": "Velocity",
        "net": {
            "type": "UNet",
            "model_channels": 96,
            "ch_mult": [
                1,
                2,
                4
            ],
            "normalization": "GroupNorm32",
            "activation": "SiLU",
            "num_res_blocks": 2,
            "zero_res_blocks": false
        },
        "sample_batch_size": 8,
        "train_batch_size": 48,
        "train_minibatch_size": 24,
        "train_images": 6000000,
        "learning_rate": 0.0002,
        "num_inference_steps": 12,
        "timestep_sampling": "logit-normal",
        "structure_pairing": false,
        "structure_candidates": 8
    },
    "saliency": {
        "type": "Saliency",
        "net": {
            "type": "ResNetSaliencyNet",
            "backbone": "resnet34",
            "num_classes": 2,
            "pretrained": true,
            "latent_channels": 512
        },
        "learning_rate": 5e-05,
        "accuracy_threshold": 0.995,
        "warmup_threshold": 0.9,
        "blend_fraction": 0.0,
        "label_smoothing": 0.1,
        "augmentations": [
            "color_jitter",
            "grayscale",
            "hflip",
            "random_erasing"
        ]
    }
}
